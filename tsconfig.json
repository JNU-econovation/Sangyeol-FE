{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "baseUrl": "."
  },
  "include": [],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    ".next",
    ".expo"
  ],
  "references": [
    {
      "path": "./packages/app"
    },
    {
      "path": "./packages/web"
    }
  ]
}